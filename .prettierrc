{
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "printWidth": 80,
  "rules": {
    "quotes": ["error", "single"],
    "no-unused-vars": ["error"],
    "no-console": "off",
    "eol-last": ["error", "always"],
    "object-curly-spacing": ["error", "always"],
    "array-bracket-spacing": ["error", "never"],
    "max-len": ["error", { "code": 120, "ignoreComments": true }],
    "comma-dangle": ["error", "only-multiline"],
    "no-dupe-keys": "error",
    "no-empty": "error"
  }
}
